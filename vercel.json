{
  "crons": [
    {
      "path": "/api/cron/enforce-billing",
      "schedule": "0 3 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/admin/login",
      "destination": "/sign-in",
      "permanent": true
    },
    {
      "source": "/admin/teams",
      "destination": "/teams",
      "permanent": true
    },
    {
      "source": "/admin/teams/:id",
      "destination": "/teams/:id",
      "permanent": true
    },
    {
      "source": "/vibe/admin/login",
      "destination": "/sign-in",
      "permanent": true
    },
    {
      "source": "/vibe/admin/teams",
      "destination": "/teams",
      "permanent": true
    },
    {
      "source": "/vibe/admin/teams/new",
      "destination": "/teams/new",
      "permanent": true
    },
    {
      "source": "/vibe/admin/teams/:id",
      "destination": "/teams/:id",
      "permanent": true
    },
    {
      "source": "/delta/teams",
      "destination": "/teams",
      "permanent": true
    },
    {
      "source": "/delta/teams/:id",
      "destination": "/teams/:id",
      "permanent": true
    },
    {
      "source": "/delta/teams/:id/new",
      "destination": "/teams/:id/delta/new",
      "permanent": true
    },
    {
      "source": "/delta/session/:id",
      "destination": "/session/:id",
      "permanent": true
    },
    {
      "source": "/t/:path*",
      "destination": "/vibe/t/:path*",
      "permanent": true
    },
    {
      "source": "/feedback/backlog",
      "destination": "/backlog",
      "permanent": true
    }
  ]
}
